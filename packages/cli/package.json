{
  "name": "@anki.md/cli",
  "version": "0.7.0",
  "description": "Anki.md CLI.",
  "author": " @timgreen",
  "bin": {
    "ankimd": "./bin/run.js"
  },
  "homepage": "https://github.com/timgreen/Anki.md",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "repository": "timgreen/Anki.md",
  "files": [
    "/bin/run.js",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@anki.md/core": "0.11.0",
    "@autoanki/anki-connect": "^0.1.2",
    "@oclif/core": "^2.0.3",
    "@oclif/plugin-autocomplete": "^2.1.2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.17",
    "@oclif/plugin-warn-if-update-available": "^2.0.23",
    "chalk": "^5.2.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "spinnies": "^0.5.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.10",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.11",
    "@types/spinnies": "^0.5.0",
    "@types/valid-url": "^1.0.3",
    "chai": "^4",
    "eslint": "^8.29.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^10",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "ankimd",
    "dirname": "ankimd",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "repositoryPrefix": "<%- repo %>/blob/cli/v<%- version %>/packages/cli/src<%- commandPath.substring(4) %>",
    "topicSeparator": " ",
    "topics": {
      "connect": {
        "description": "Cli wrapper for AnkiConnect."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/timgreen/Anki.md/issues",
  "keywords": [
    "oclif",
    "anki",
    "markdown"
  ],
  "types": "dist/index.d.ts"
}
